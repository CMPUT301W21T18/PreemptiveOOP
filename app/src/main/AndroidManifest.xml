<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.preemptiveoop">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.PreemptiveOOP">
        <activity android:name=".experiment.DisplayExpStats"></activity>
        <activity android:name=".experiment.DisplayExpGraphs" ></activity>

        <activity android:name=".trial.ExecuteTrial" />
        <activity android:name=".experiment.ExperimentList" />
        <activity android:name=".user.UserRegister" />
        <activity
            android:name=".ui.login.UserRegister"
            android:label="@string/title_activity_user_register" />
        <activity android:name=".user.UserLogin" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>